@using EindwerkCarParkingLib
@model IEnumerable<Parking>
<h1>Parkings</h1>
    @foreach (var p in Model)
    {
        <div class="col-md-3">
            <div class=" well well-lg">
                <h4>Parking: </h4>
                <ul>
                    <li>Naam: @p.ParkingNaam</li>
                    <li>locatie: @p.Locatie.Gemeente.GemeenteNaam</li>
                    <li>straat: @p.Locatie.Straat</li>
                    <li>nummer: @p.Locatie.Nr</li>
                    <li>land: @p.Locatie.Gemeente.Land.LandNaam</li>
                </ul>
            </div>
        </div>
    }